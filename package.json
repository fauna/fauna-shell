{
  "name": "fauna-shell",
  "description": "faunadb shell",
  "version": "2.0.2",
  "author": "Fauna",
  "bin": {
    "fauna": "./yargs-entrypoint.js"
  },
  "bugs": "https://github.com/fauna/fauna-shell/issues",
  "dependencies": {
    "@inquirer/prompts": "^3.1.1",
    "@oclif/core": "^4.0.12",
    "@oclif/plugin-help": "^5.2.14",
    "@oclif/plugin-plugins": "^2.4.7",
    "awilix": "^11.0.0",
    "chalk": "^4.1.2",
    "cli-table": "^0.3.1",
    "cli-ux": "^4.8.0",
    "csv-parse": "^5.0.4",
    "csv-stream": "^0.2.0",
    "dotenv": "^8.2.0",
    "escodegen": "^1.12.0",
    "esprima": "^4.0.1",
    "exponential-backoff": "^3.1.0",
    "faunadb": "^4.5.4",
    "globby": "^11.0.0",
    "ini": "^1.3.5",
    "inquirer": "^8.1.1",
    "moment": "^2.29.1",
    "object-sizeof": "^1.6.1",
    "open": "8.4.2",
    "rate-limiter-flexible": "^2.3.6",
    "stream-json": "^1.7.3",
    "supports-color": "^8",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@cloudcmd/stub": "^4.0.1",
    "@inquirer/testing": "^2.1.7",
    "@oclif/test": "^4.0.4",
    "@types/chai": "^4.3.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.6.0",
    "@types/sinon": "^17.0.3",
    "@types/supports-color": "^8.1.3",
    "@typescript-eslint/parser": "6.7.3",
    "c8": "^8.0.1",
    "eslint": "^8.10.0",
    "eslint-config-oclif": "^1.5.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "chai": "^5.1.1",
    "husky": "^7.0.4",
    "jest": "^29.7.0",
    "mocha": "^10.7.3",
    "mock-require": "^3.0.3",
    "nock": "^14.0.0-beta.8",
    "oclif": "^3.9.2",
    "prettier": "^2.3.0",
    "sinon": "^19.0.2",
    "sinon-called-with-diff": "^3.1.1",
    "sinon-chai": "^4.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "word-wrap": "^1.2.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/oclif.manifest.json",
    "/src",
    "/dist"
  ],
  "homepage": "https://github.com/fauna/fauna-shell",
  "keywords": [
    "fauna",
    "faunadb",
    "shell",
    "database",
    "nosql",
    "oclif"
  ],
  "license": "MPL-2.0",
  "main": "dist/index.js",
  "oclif": {
    "helpClass": "dist/custom-help",
    "bin": "fauna",
    "dirname": "fauna",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "topics": {
      "endpoint": {
        "description": "Manage endpoints in ~/.fauna-shell."
      },
      "schema": {
        "description": "Manage database schema."
      },
      "project": {
        "description": "Manage project settings in .fauna-project."
      },
      "environment": {
        "description": "Manage environments in the current project."
      }
    }
  },
  "repository": "fauna/fauna-shell",
  "scripts": {
    "build": "rm -rf dist && tsc -b",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest",
    "pretest": "yarn fixlint",
    "local": "export $(cat .env | xargs); node bin/run",
    "local-test": "export $(cat .env | xargs); mocha \"test/**/*.test.{js,ts}\"",
    "lint": "eslint .",
    "fixlint": "eslint . --fix",
    "test-old": "c8 -r html mocha --forbid-only \"test/**/*.test.{js,ts}\"",
    "test": "mocha --recursive ./yargs-test --require ./yargs-test/mocha-root-hooks.mjs",
    "version": "oclif-dev readme && git add README.md",
    "fmt": "prettier -w src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretest"
    }
  }
}
