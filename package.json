{
  "name": "fauna-shell",
  "description": "faunadb shell",
  "version": "3.0.0-beta",
  "author": "Fauna",
  "type": "module",
  "bin": {
    "fauna": "./yargs-entrypoint.mjs"
  },
  "bugs": "https://github.com/fauna/fauna-shell/issues",
  "dependencies": {
    "@inquirer/prompts": "^3.1.1",
    "awilix": "^12.0.2",
    "chalk": "^5.3.0",
    "eslint": "^9.12.0",
    "faunadb": "^4.5.4",
    "inquirer": "^8.1.1",
    "open": "10.1.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.1",
    "@inquirer/testing": "^2.1.7",
    "@types/chai": "^4.3.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.6.0",
    "@types/sinon": "^17.0.3",
    "@types/yargs": "^17.0.33",
    "chai": "^5.1.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "globals": "^15.10.0",
    "husky": "^7.0.4",
    "mocha": "^10.7.3",
    "mocha-junit-reporter": "^2.2.1",
    "prettier": "^2.3.0",
    "sinon": "^19.0.2",
    "sinon-called-with-diff": "^3.1.1",
    "sinon-chai": "^4.0.0",
    "try-to-catch": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/yargs-entrypoint.mjs",
    "/bin",
    "/src",
    "/dist"
  ],
  "homepage": "https://github.com/fauna/fauna-shell",
  "keywords": [
    "fauna",
    "faunadb",
    "shell",
    "database",
    "nosql",
    "cli"
  ],
  "license": "MPL-2.0",
  "main": "dist/index.js",
  "repository": "fauna/fauna-shell",
  "scripts": {
    "build": "rm -rf dist && tsc -b",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest",
    "pretest": "yarn fixlint",
    "local-test": "mocha --recursive ./yargs-test --require ./yargs-test/mocha-root-hooks.mjs",
    "lint": "eslint .",
    "fixlint": "eslint . --fix",
    "test": "mocha --recursive ./yargs-test --require ./yargs-test/mocha-root-hooks.mjs --reporter spec --reporter mocha-junit-reporter",
    "fmt": "prettier -w src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretest"
    }
  }
}
